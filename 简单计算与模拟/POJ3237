#include<iostream>
using namespace std;
int main()
{
    int n,a;
    cin>>n;
    for(int i=0;i<n;i++)
    {
        cin>>a;
        if(a%4==0) 
        {
            cout<<a/4<<" "<<a/2<<endl;
        }
        else if(a%2==0)
        {
            cout<<a/4+1<<" "<<a/2<<endl;
        }
        else
        {
            cout<<0<<" "<<0<<endl;
        }
    }
    return 0;
}
